    "version": "0.2.0",
    "configurations": [

        {
            "name": "groww_app_design web",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
               " -d chrome --no-debug",
                "--web-experimental-hot-reload"
            ]
        },
        {
            "name": "groww_app_design",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "groww_app_design (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "groww_app_design (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}